#!/usr/bin/env node

const program = require('commander')
const checkVersion = require('../lib/checkVersion')
const pkg = require('../package')

checkVersion(() => {
  program
    .version(pkg.version)
    .usage('<command> <option>')
    .command('init', '初始化一个项目')
    .command('mock', 'Mock服务器')
    .parse(process.argv)
})
